{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "ShareableOwner",
	"type": "object",
	"properties": {
		"owner": {
			"type": "object",
			"properties": {
				"dids": {
					"type": "array",
					"items": {
						"type": "string"
					}
				},
				"id": {
					"type": "string"
				},
				"secret": {
					"type": "string"
				}
			},
			"required": ["dids", "secret"]
		},
		"migrate_to": {
			"description": "The Stream ID of the ShareableOwner Stream that this Stream has migrated to.",
			"type": "string"
		}
	}
}
